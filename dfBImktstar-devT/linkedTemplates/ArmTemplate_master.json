{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfBImktstar-devT"
		},
		"datalakemktStart_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalakemktStart'"
		},
		"srv_bi_market_star_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'srv_bi_market_star'"
		},
		"datalakemktStart_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://samarketstarunit.dfs.core.windows.net/"
		},
		"srv_bi_market_star_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "srv-unit-market-star.database.windows.net"
		},
		"srv_bi_market_star_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-bi-market-star"
		},
		"srv_bi_market_star_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminsql"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfBImktstar-devT_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalakemktStart_accountKey": {
						"value": "[parameters('datalakemktStart_accountKey')]"
					},
					"srv_bi_market_star_password": {
						"value": "[parameters('srv_bi_market_star_password')]"
					},
					"datalakemktStart_properties_typeProperties_url": {
						"value": "[parameters('datalakemktStart_properties_typeProperties_url')]"
					},
					"srv_bi_market_star_properties_typeProperties_server": {
						"value": "[parameters('srv_bi_market_star_properties_typeProperties_server')]"
					},
					"srv_bi_market_star_properties_typeProperties_database": {
						"value": "[parameters('srv_bi_market_star_properties_typeProperties_database')]"
					},
					"srv_bi_market_star_properties_typeProperties_userName": {
						"value": "[parameters('srv_bi_market_star_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "dfBImktstar-devT_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dfBImktstar-devT_ArmTemplate_0')]"
			]
		},
		{
			"name": "dfBImktstar-devT_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dfBImktstar-devT_ArmTemplate_1')]"
			]
		},
		{
			"name": "dfBImktstar-devT_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'dfBImktstar-devT_ArmTemplate_2')]"
			]
		}
	]
}